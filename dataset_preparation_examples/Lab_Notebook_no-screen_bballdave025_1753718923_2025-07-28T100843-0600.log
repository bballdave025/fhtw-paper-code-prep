funfufoo()
{
  this_arg=$1
  
  retval=0
  
  this_param=
  
  shift

  if [ "${this_arg}" = "--help" -o "${this_arg}" = "-h" ]; then
    echo "returning 0" >&2
    return ${retval}
  else
    this_param="${this_arg}"
    if ${#@}; then
      echo " Extra parameters:" >&2
      echo "$@" >&2
      echo " will be ignored." >&2
    fi
    if [ ! -z "${this_param}" ]; then
      if [ -f "${this_param}" ]; then
        echo " Opening" >&2
        echo "${this_param}" >&2
        echo " in Notepad++" >&2
      else
        echo "Your parameter is not a file." >&2
        retval=1
      fi
    else
      echo " Opening Notepad++" >&2
    fi
  fi
  if [ ${retval} -eq 0 ]; then
    exe_filepath='/cygdrive/c/Program Files (x86)/Notepad++/notepad++'
    echo " Using"
    echo "${exe_filepath} ${this_param} &"
  fi
  
  return ${retval}
}



